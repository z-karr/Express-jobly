{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobNew.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 50
      },
      "salary": {
        "type": "number",
        "minimum": 0
      },
      "equity": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "companyHandle": {
        "type": "string",
        "minLength": 1,
        "maxLength": 10
      }
    },
    "additionalProperties": false,
    "required": [
      "title",
      "salary",
      "equity",
      "companyHandle"
    ]
  }